<!--pages/wxs/wxs.wxml-->
<wxs module="info">
  var message = "Hello World";
  var name = "coderwhy";
  var sum = function(num1,num2){
    return num1 + num2
  }
  //commonjs的模块化导出的写法
  module.exports = {
    message:message,
    name:name,
    sum:sum
  }
</wxs>
<view>{{info.message}}</view>
<view>{{info.name}}</view>
<view>{{info.sum(20,30)}}</view>
